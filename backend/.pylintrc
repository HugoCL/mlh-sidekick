[MAIN]
py-version = 3.12
ignore = .venv, venv, .direnv, __pycache__
ignore-paths = prisma/.*
jobs = 0

[MESSAGES CONTROL]
disable =
    missing-module-docstring,
    missing-class-docstring,
    missing-function-docstring,
    too-few-public-methods,
    fixme

[REPORTS]
output-format = text
score = no

[FORMAT]
max-line-length = 100

[BASIC]
good-names = i, j, k, _, id, pk, db, app

[TYPECHECK]
ignored-modules = prisma
